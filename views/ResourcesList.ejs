<%-include("header")-%>

<div class="box" id="heading">
  <h1> Links to all the resources for</h1>
</div>

  <%for(var i=0;i<resource.length;++i){%>
    <div class="box" id="heading">
    <h3><%=resource[i].name%></h3>
    </div>
    <div class="box">
  <div class="item">
    <ul>
      <%for(var j=0;j<resource[i].resources.length;++j){%>
      <li><p><a href=<%=resource[i].resources[j]%>><%=resource[i].resources[j]%></a></p></li>
      <%}%>
        </ul>
      </div>
        </div>
    <%}%>


  <%-include("footer")-%>
